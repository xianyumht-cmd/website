{
  "extends": ["html-validate:recommended"],
  "rules": {
    "no-trailing-whitespace": "off",
    "no-conditional-comment": "off",
    "doctype-style": "off",
    "no-inline-style": "off",
    "no-inline-script": "off",
    "no-style-tag": "off",
    "no-script-style-type": "off",
    "require-sri": "off",
    "void-style": "off",
    "wcag/h30": "off",
    "wcag/h37": "off",
    "wcag/h67": "off"
  }
}

